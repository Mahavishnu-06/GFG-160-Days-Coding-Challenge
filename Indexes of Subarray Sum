
class Solution {
    static ArrayList<Integer> subarraySum(int[] arr, int target) {
       int sum=0,n=arr.length,left=0,right=0;
       while(right<n){
            sum=sum+arr[right];
            while(sum>target){
                sum=sum-arr[left];
                left++;
            }if(sum==target){
                return new ArrayList<>(Arrays.asList(left+1,right+1));
            }
            right++;
       }
       return new ArrayList<>(Arrays.asList(-1));
    }
}
