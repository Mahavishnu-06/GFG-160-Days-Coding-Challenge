class Solution {
    public char nonRepeatingChar(String s) {
        char ch[]=s.toCharArray();
        HashMap<Character,Integer> freq=new HashMap<>();
        for(char c:ch){
            freq.put(c,freq.getOrDefault(c,0)+1);
        }
        for(char c:ch){
            if(freq.get(c)==1){
                return c;
            }
        }
        return '$';
    }
}
