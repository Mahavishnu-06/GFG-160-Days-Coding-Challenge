class Solution {
    public int getSecondLargest(int[] arr) {
        ArrayList<Integer> ans=new ArrayList<>();
        for(int i:arr){
            ans.add(i);
        }
        int firstmax= Collections.max(ans);
        ans.removeAll(Collections.singleton(firstmax));
        if(ans.isEmpty()){
            return -1;
        }
        return Collections.max(ans);
    }
}
